      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GetRegisterUser">
    <xs:complexType>
      <xs:sequence>
        <xs:element xmlns:q2="http://schemas.datacontract.org/2004/07/Service.Database" minOccurs="0" name="user" nillable="true" type="q2:User" />
